@model List<WEB2.Models.Domein.Employee>

<h1>List of Employees</h1>
<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Salary</th>
        <th>DateOfBirth</th>
        <th>Department</th>
        <th>Redact a data</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>
                @employee.Id
            </td>

            <td>
                @employee.Name
            </td>
            <td>
                @employee.Email
            </td>
            <td>
                @employee.Salary
            </td>
            <td>
                @employee.DateOfBirth.ToString("yyyy MMMM dd")
            </td>
            <td>
                @employee.Department
            </td>
            <td><a href="Employees/Content/@employee.Id">Content</a></td>
        </tr>
    }
    </tbody>
</table>